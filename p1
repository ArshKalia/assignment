using System;

class Program
{
    static void Main()
    {
        
        Console.Write("Please provide your age: ");
        int age = Convert.ToInt32(Console.ReadLine());

        
        if (age >= 18)
        {
            Console.WriteLine("You qualify as an adult");
        }
        else
        {
            Console.WriteLine("You are considered a minor");
        }
    }
}
